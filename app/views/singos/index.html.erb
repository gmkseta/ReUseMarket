<p id="notice"><%= notice %></p>

<h1>Singos</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th>Item</th>
      <th>Sel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @singos.each do |singo| %>
      <tr>
        <td><%= singo.content %></td>
        <td><%= singo.user %></td>
        <td><%= singo.item %></td>
        <td><%= singo.sel %></td>
        <td><%= link_to 'Show', singo %></td>
        <td><%= link_to 'Edit', edit_singo_path(singo) %></td>
        <td><%= link_to 'Destroy', singo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Singo', new_singo_path %>
